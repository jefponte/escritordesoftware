<?php

/**
 * Classe feita para manipulação do objeto endereco 
 * feita automaticamente com programa gerador de software inventado por						
 * Autor Jefferson Uchôa Ponte
 *						
 *
 */												
class Endereco
{


	private $Rua;

	private $Numero;

	private $Bairro;

	private $Uf;

	private $Estado;

	public function setRua($rua)
	{
		$this->Rua = $rua;

	}

	public function getRua($rua)
	{
		return $this->Rua;

	}


	public function setNumero($numero)
	{
		$this->Numero = $numero;

	}

	public function getNumero($numero)
	{
		return $this->Numero;

	}


	public function setBairro($bairro)
	{
		$this->Bairro = $bairro;

	}

	public function getBairro($bairro)
	{
		return $this->Bairro;

	}


	public function setUf($uf)
	{
		$this->Uf = $uf;

	}

	public function getUf($uf)
	{
		return $this->Uf;

	}


	public function setEstado($estado)
	{
		$this->Estado = $estado;

	}

	public function getEstado($estado)
	{
		return $this->Estado;

	}


}
?>